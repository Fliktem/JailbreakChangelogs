<!DOCTYPE html>
<html lang="en" data-bs-theme="dark">
  <head>
    <!-- Character encoding -->
    <meta charset="UTF-8" />

    <!-- Responsive design -->
    <meta name="viewport" content="width=device-width, initial-scale=1" />

    <!-- Primary Meta Tags -->
    <meta name="title" content="<%= title %> | Jailbreak Changelogs" />
    <meta
      name="description"
      content="The page you were looking for could not be found. Return to the homepage to continue browsing Jailbreak changelogs and updates."
    />
    <meta name="author" content="Jailbreak Changelogs" />
    <meta name="referrer" content="no-referrer" />

    <!-- Open Graph / Facebook -->
    <meta property="og:type" content="website" />
    <meta property="og:url" content="https://jailbreakchangelogs.xyz" />
    <meta property="og:title" content="<%= title %> | Jailbreak Changelogs" />
    <meta
      property="og:description"
      content="The page you were looking for could not be found. Return to the homepage to continue browsing Jailbreak changelogs and updates."
    />
    <meta
      property="og:image"
      content="https://jailbreakchangelogs.xyz/assets/logos/Banner_Background.webp"
    />

    <!-- Twitter -->
    <meta name="twitter:card" content="summary_large_image" />
    <meta name="twitter:url" content="https://jailbreakchangelogs.xyz" />
    <meta name="twitter:title" content="<%= title %> | Jailbreak Changelogs" />
    <meta
      name="twitter:description"
      content="The page you were looking for could not be found. Return to the homepage to continue browsing Jailbreak changelogs and updates."
    />
    <meta
      name="twitter:image"
      content="https://jailbreakchangelogs.xyz/assets/logos/Banner_Background.webp"
    />

    <!-- Theme Color -->
    <meta name="theme-color" content="#124e66" />

    <!-- Favicon -->
    <link rel="icon" type="image/x-icon" href="/favicon.ico" />

    <!-- CSS Links -->
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH"
      crossorigin="anonymous"
    />
    <link
      rel="stylesheet"
      href="https://cdn.jsdelivr.net/gh/JBChangelogs/JailbreakChangelogs@1.0.19/styles/colors.min.css"
    />
    <link
      rel="stylesheet"
      href="https://cdn.jsdelivr.net/gh/JBChangelogs/JailbreakChangelogs@1.0.18/styles/main.min.css"
    />
    <link
      rel="stylesheet"
      href="https://cdn.jsdelivr.net/gh/JBChangelogs/JailbreakChangelogs@1.0.19/styles/404.min.css"
    />
    <!-- <link rel="stylesheet" href="styles/404.css" /> -->

    <!-- Google Fonts -->
    <link rel="preconnect" href="https://fonts.googleapis.com" />
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
    <link
      href="https://fonts.googleapis.com/css2?family=Bangers&family=Luckiest+Guy&display=swap"
      rel="stylesheet"
    />

    <title><%= title %> | Jailbreak Changelogs</title>
  </head>

  <body>
    <%- include('partials/nav') %>

    <div class="error-page container text-center mt-5">
      <img
        src="/assets/logos/JB Changelogs Banner.webp"
        alt="404 Logo"
        class="img-fluid mb-4"
        style="max-width: 300px"
      />
      <h1>404</h1>
      <h2>Page Not Found</h2>
      <p class="lead">
        The page you are looking for might have been removed, had its name
        changed, or is temporarily unavailable.
      </p>
      <a href="/" class="btn btn-primary mt-3">Return Home</a>
    </div>

    <%- include('partials/footer') %>
    <!-- Bootstrap Bundle -->
    <script
      src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"
      integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz"
      crossorigin="anonymous"
    ></script>

    <!-- Custom Scripts -->
    <script src="https://cdn.jsdelivr.net/gh/JBChangelogs/JailbreakChangelogs@1.0.18/scripts/main.min.js"></script>
    <!-- <script src="scripts/main.js"></script> -->

    <script
      async
      src="https://pagead2.googlesyndication.com/pagead/js/adsbygoogle.js?client=ca-pub-8152532464536367"
      crossorigin="anonymous"
    ></script>
    <script
      async
      src="https://fundingchoicesmessages.google.com/i/pub-8152532464536367?ers=1"
    ></script>
    <script>
      (function () {
        function signalGooglefcPresent() {
          if (!window.frames["googlefcPresent"]) {
            if (document.body) {
              const iframe = document.createElement("iframe");
              iframe.style =
                "width: 0; height: 0; border: none; z-index: -1000; left: -1000px; top: -1000px;";
              iframe.style.display = "none";
              iframe.name = "googlefcPresent";
              document.body.appendChild(iframe);
            } else {
              setTimeout(signalGooglefcPresent, 0);
            }
          }
        }
        signalGooglefcPresent();
      })();
    </script>
  </body>
</html>
